{
  "name": "orphan",
  "version": "1.0.0",
  "description": "Orphan Radio",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client-install": "npm install --prefix client && npm run build --prefix client",
    "heroku-postbuild": "NODE_ENV=production cd client && npm install && npm run build "
  },
  "engines": {
    "node": "11",
    "npm": "6.9.0"
  },
  "author": "Mike Cassidy",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.3",
    "express-form-data": "^2.0.8",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.2",
    "nodemon": "^1.19.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-scripts": "^3.0.1",
    "react-transition-group": "^4.2.0",
    "three-orbitcontrols": "^2.102.1",
    "validator": "^9.4.1"
  }
}
